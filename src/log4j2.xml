<?xml version="1.0" encoding="UTF-8"?> 
<configuration status="OFF"> 
  <appenders> 
    <Console name="Console" target="SYSTEM_OUT"> 
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/> 
    </Console>
    <File name="File-Known" fileName="log/Known.log"> 
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/> 
    </File>
    <File name="File-Scheduler" fileName="log/Scheduler.log"> 
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/> 
    </File>
  </appenders> 
  <loggers>
    <logger name="cn.edu.fudan.admis.mt.schedule.Known" level="DEBUG" additivity="false">  
      <appender-ref ref="File-Known"/>  
    </logger>
    <logger name="cn.edu.fudan.admis.mt.schedule.Scheduler" level="DEBUG" additivity="false">  
      <appender-ref ref="File-Scheduler"/>  
    </logger>
    <root level="INFO"> 
      <appender-ref ref="Console"/> 
    </root> 
  </loggers> 
</configuration>